{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": 0,
    "import/prefer-default-export": 0,
    "prettier/prettier": "error",
    "no-underscore-dangle": 0,
    "camelcase": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
